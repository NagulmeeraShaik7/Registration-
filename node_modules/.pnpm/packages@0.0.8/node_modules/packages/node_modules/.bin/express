#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/theia/myapp/node_modules/.pnpm/express@3.4.8/node_modules/express/bin/node_modules:/home/theia/myapp/node_modules/.pnpm/express@3.4.8/node_modules/express/node_modules:/home/theia/myapp/node_modules/.pnpm/express@3.4.8/node_modules:/home/theia/myapp/node_modules/.pnpm/node_modules:/home/theia/myapp/node_modules:/home/theia/node_modules:/home/node_modules:/node_modules:/home/theia/myapp/node_modules/.pnpm/packages@0.0.8/node_modules/packages/node_modules:/home/theia/myapp/node_modules/.pnpm/packages@0.0.8/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/theia/myapp/node_modules/.pnpm/express@3.4.8/node_modules/express/bin/node_modules:/home/theia/myapp/node_modules/.pnpm/express@3.4.8/node_modules/express/node_modules:/home/theia/myapp/node_modules/.pnpm/express@3.4.8/node_modules:/home/theia/myapp/node_modules/.pnpm/node_modules:/home/theia/myapp/node_modules:/home/theia/node_modules:/home/node_modules:/node_modules:/home/theia/myapp/node_modules/.pnpm/packages@0.0.8/node_modules/packages/node_modules:/home/theia/myapp/node_modules/.pnpm/packages@0.0.8/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../express@3.4.8/node_modules/express/bin/express" "$@"
else
  exec node  "$basedir/../../../../../express@3.4.8/node_modules/express/bin/express" "$@"
fi
